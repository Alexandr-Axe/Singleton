<Window x:Class="Singleton.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Singleton"
        mc:Ignorable="d"
        Title="MainWindow" MinWidth="365" MaxWidth="365" MinHeight="330" MaxHeight="330">
    <Grid>
        <TextBox x:Name="TBInformace" FontSize="25" HorizontalAlignment="Left" Height="170" TextWrapping="Wrap" VerticalAlignment="Top" Width="330" Margin="10,10,0,0"/>
        <Button x:Name="BTNInformace" Content="Vypiš informace" HorizontalAlignment="Left" VerticalAlignment="Top" Width="100" Height="100" Margin="140,185,0,0" Click="Button_Click"/>
    </Grid>
</Window>
